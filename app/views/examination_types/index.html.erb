<div class="link2"><%= link_to 'Inregistrare noua', new_examination_type_path %></div><br/>
<div class="table-users2">
<div class="header">Lista tipuri examinare</div>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
      <th>Tip examinare</th>
      <th>Titlu disciplina</th>
    </tr>
  </thead>

  <tbody>
    <% @examination_types.each do |examination_type| %>
      <tr>
        <td><%= link_to 'Detalii', examination_type %></td>
        <td><%= link_to 'Modificare', edit_examination_type_path(examination_type) %></td>
        <td><%= link_to 'Stergere', examination_type, method: :delete, data: { confirm: 'esti sigur ca vrei sa stergi' } %></td>
        <td><%= examination_type.name %></td>
        <td><%= examination_type.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

